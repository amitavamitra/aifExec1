<%- include('partials/header'); -%>
<body>
  <h1>Login Page: </h1>
 <form action="/login" method="post">
<br>
<hr>
<h3>AI-Instance Credential</h3>
<br>
          <div class="form-row">
            <div class="form-group row">
              <label  class="col-sm-3 col-form-label"  for="ainstance">ai-core-instance</label>
              <div class="col-sm-8">
                <input type="text"  name="ainstance" id="ainstance" 
                class="form-control col-sm-8" placeholder="Reading it from enblement-sk.json">
                <!-- value="<%=bucket%>"> -->
              </div>
            </div>
   <br>
<hr>
<h3>s3-Object Store Credential</h3>
<br>
    <div class="form-row">
        <div class="form-group row">
          <label  class="col-sm-3 col-form-label"  for="bucket">s3-bucket</label>
          <div class="col-sm-8">
            <input type="text"  name="bucket" id="bucket" 
            class="form-control col-sm-8 " placeholder="s3-bucket">
            <!-- value="<%=bucket%>"> -->
          </div>
        </div>
        <div class="form-group row">
          <label  class="col-sm-3 col-form-label"  for="access">s3-accessKey</label>
          <div class="col-sm-8">
          <input type="password" name="accesskey" id="access" 
          class="form-control col-sm-8" placeholder="s3-accessKey">
          <!-- value="<%=access_key_id%>"> -->
          </div>
        </div>
        <div class="form-group row">
          <label  class="col-sm-3 col-form-label"  for="access">s3-secretKey</label>
          <div class="col-sm-8">
            <input type="password" name="secret" id="secret"  
            class="form-control  col-sm-8" placeholder="s3-secret">
            <!-- value="<%=secret_access_key%>"> -->
          </div>
        </div>
        <br><hr>
    </div>
      <button  id="login" type="submit" class="btn btn-primary btn-lg   pull-right">Login</button>
</form>



</body>
